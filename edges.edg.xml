<edges>
   <!-- Вертикаль (Ось Y) - ГЛАВНАЯ дорога - 3 полосы -->
   <!-- numLanes="3" speed="16.6" (это 60 км/ч) -->
   <edge id="n_to_c" from="N" to="C" priority="2" numLanes="3" speed="16.6" />
   <edge id="c_to_n" from="C" to="N" priority="2" numLanes="3" speed="16.6" />
   
   <edge id="s_to_c" from="S" to="C" priority="2" numLanes="3" speed="16.6" />
   <edge id="c_to_s" from="C" to="S" priority="2" numLanes="3" speed="16.6" />

   <!-- Горизонталь (Ось X) - ВТОРОСТЕПЕННАЯ дорога - 1 полоса -->
   <edge id="e_to_c" from="E" to="C" priority="1" numLanes="1" speed="16.6" />
   <edge id="c_to_e" from="C" to="E" priority="1" numLanes="1" speed="16.6" />

   <edge id="w_to_c" from="W" to="C" priority="1" numLanes="1" speed="16.6" />
   <edge id="c_to_w" from="C" to="W" priority="1" numLanes="1" speed="16.6" />
</edges>